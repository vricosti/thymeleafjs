{
    "name": "thymeleafjs",
    "version": "1.0.0",
    "type": "module",
    "description": "A simple template engine for JavaScript that supports the Thymeleaf syntax",
    "main": "dist/thymeleaf.js",
    "scripts": {
        "build": "webpack --config webpack.config.js && ncp dist public/dist",
        "test": "node src/node.js",
        "build:browser": "parcel build src/thymeleaf.browser.js --dist-dir dist && ncp dist public/dist",
        "build:node": "parcel build src/thymeleaf.js --dist-dir dist --target node"
    },
    "keywords": [
        "thymeleaf",
        "template",
        "engine",
        "javascript"
    ],
    "author": "Your Name",
    "license": "MIT",
    "dependencies": {
        "dom-compare": "^0.6.0",
        "jsdom": "^21.1.1",
        "prettier": "^2.8.8"
    },
    "devDependencies": {
        "@babel/cli": "^7.21.5",
        "@babel/core": "^7.21.8",
        "@babel/preset-env": "^7.21.5",
        "babel-loader": "^9.1.2",
        "jest": "^27.4.7",
        "ncp": "^2.0.0",
        "webpack-cli": "^5.1.0"
    }
}
